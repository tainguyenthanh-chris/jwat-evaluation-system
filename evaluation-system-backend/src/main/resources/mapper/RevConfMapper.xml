<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.clt.evaluation_system_backend.mapper.RevConfMapper">
    <select id="selectAll" resultType="com.clt.evaluation_system_backend.dto.response.config.RevConfResponse">
        SELECT
            rev_conf_id AS revConfId,
            rev_conf_cd AS revConfCd,
            rev_conf_type AS revConfType,
            rev_conf_roles AS revConfRolesJson
        FROM rev_conf
        WHERE del_flg = 'F'
    </select>
</mapper>
